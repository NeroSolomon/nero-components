{
  "root": true,
  "extends": [
    "plugin:prettier/recommended",
    "prettier",
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "globals": {
  }
}
